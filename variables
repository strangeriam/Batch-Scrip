@echo off 
set message=Hello World 
echo %message%

# Output: Hello World


# ====================
# Hold a numeric value

@echo off 
SET /A a = 5 
SET /A b = 10 
SET /A c = %a% + %b% 
echo %c% 
SET /A c = %a% - %b% 
echo %c% 
SET /A c = %b% / %a% 
echo %c% 
SET /A c = %b% * %a% 
echo %c%

# Output:
15
-5
2
50
